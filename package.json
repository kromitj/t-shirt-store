    "classnames": "^2.2.6",
    "concurrency": "^0.1.4",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "dotenv": "^6.2.0",
    "express": "~4.16.0",
    "forest-express-sequelize": "^2.17.2",
    "gravatar": "^1.8.0",
    "jsonwebtoken": "^8.5.0",
    "morgan": "~1.9.0",
    "mysql": "^2.16.0",
    "mysql2": "^1.6.5",
    "node-cache": "^4.2.0",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.10",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pg": "^7.9.0",
    "request-ip": "^2.1.3",
    "sequelize": ">=3.20.0",
    "sequelize-cli": "^5.4.0",
    "sinon": "^7.2.6",
